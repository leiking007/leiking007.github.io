  "scripts": {
    "build": "run-s build:*",
    "build:css": "npx  @tailwindcss/cli -i ./assets/css/main.css -o ./assets/css/style.css -m",
    "build:hugo": "hugo --minify --source ./../../",
    "build:pagefind": "npx pagefind --site ../../public --output-subdir search",
    "dev": "run-p dev:*",
    "dev:css": "npx  @tailwindcss/cli -i ./assets/css/main.css -o ./assets/css/style.css -w",
    "dev:hugo": "hugo server --source ./../../ --disableFastRender --gc --noHTTPCache --noBuildLock --logLevel debug --port 1313 --bind 0.0.0.0",
    "dev:pagefind": "npx pagefind --site ../../public --output-subdir search"
  },
  "dependencies": {
